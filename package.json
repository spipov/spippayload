{
  "name": "",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3200",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3200",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3200",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.50.0",
    "@payloadcms/email-nodemailer": "^3.50.0",
    "@payloadcms/next": "3.50.0",
    "@payloadcms/payload-cloud": "3.50.0",
    "@payloadcms/plugin-seo": "^3.50.0",
    "@payloadcms/richtext-lexical": "3.50.0",
    "@payloadcms/ui": "3.50.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "drizzle-orm": "^0.44.4",
    "graphql": "^16.11.0",
    "next": "15.4.4",
    "nodemailer": "^7.0.5",
    "payload": "3.50.0",
    "postgres": "^3.4.7",
    "react": "19.1.1",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "drizzle-kit": "^0.31.4",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
